{
  "name": "react-bootstrap-form",
  "version": "0.1.0",
  "description": "A dynamic form for React styled with bootstrap",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-cjs && npm run build-es6",
    "build-cjs": "rimraf lib && babel ./src -d lib --ignore '__tests__'",
    "build-es6": "rimraf es6 && babel ./src -d es6 --blacklist=es6.modules --ignore '__tests__'",
    "build-umd": "NODE_ENV=production webpack src/index.js umd/ReactRouter.js",
    "build-min": "NODE_ENV=production webpack -p src/index.js umd/ReactRouter.min.js",
    "lint": "eslint src examples",
    "start": "node examples/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharingapples/react-bootstrap-form.git"
  },
  "author": "Ranjan Shrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sharingapples/react-bootstrap-form/issues"
  },
  "homepage": "https://github.com/sharingapples/react-bootstrap-form#readme",
  "dependencies": {
    "moment": "^2.12.0",
    "react": "^0.14.7"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.4.0",
    "eslint-plugin-react": "^4.2.3",
    "estraverse-fb": "^1.3.1",
    "express": "^4.13.4",
    "express-urlrewrite": "^1.2.0",
    "react-dom": "^0.14.7",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1"
  }
}
